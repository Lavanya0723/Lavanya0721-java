/*output:
lavanya!@#$
!->33
@->64
#->35
$->36*/

import java.util.*;
class SpecialCharacters{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String  s=sc.nextLine();
        for(int i=0;i<s.length();i++){
            char c=s.charAt(i);
            if((int)c>=33 && (int)c<=64){
                System.out.println((char)c +"->" + (int)c);
            }
        }
    }
}
