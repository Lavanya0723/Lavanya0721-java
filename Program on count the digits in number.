import java.util.*;
class Count
{
 public static void main(String args[])
  {
  Scanner sc=new Scanner(System.in);
  System.out.print("Enter n value:");
  int n=sc.nextInt();
  int count=0;
  int rem=0;
  while(n!=0)
  {
   rem=n%10;
   n=n/10;
   count=count+1;
  }
 System.out.println(count);
}
}
  
