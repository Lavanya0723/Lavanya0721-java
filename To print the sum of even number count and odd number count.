/*output:10
20
25*/
import java.util.*;
class SumEvenOdd{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int  e=0;
        int o=0;
        for(int i=0;i<n;i++){
            if(i%2==0){
                e=e+i;
            }
            else{
                o=o+i;
            }  
        }
        System.out.println(e);
        System.out.println(o);

    }
}
