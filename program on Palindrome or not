import java.util.*;
class Palindrome
{
 public static void main(String args[])
 {
  Scanner sc=new Scanner(System.in);
  System.out.print("Enter the n value:");
  int n=sc.nextInt();
  int rev=0;
  int rem=0;
  int temp=n;
  while(n!=0)
  {
   rem=n%10;
  n=n/10;
  rev=rev*10+rem; 
 }
   if(temp==rev)
   {
  
   System.out.println("Palindrome number is  " +temp);
    
   }
   else
   {
   System.out.println(temp+ " not palindrome number");
    
   }
}
}
