/*output:
Enter a String :Lavanya
aynavaL
*/
import java.util.*;
class ReverseString{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a String :");
        String s=sc.nextLine();
        for(int i=s.length()-1;i>=0;i--){
            char c=s.charAt(i);
            System.out.print((char)c);

        }
    }
}
