import java.util.*;
class Swap
{
public static void main(String args[])
{
 Scanner sc=new Scanner(System.in);
 System.out.print("Enter n value:");
 int n=sc.nextInt();
 System.out.print("Enter m value:");
 int m=sc.nextInt();
 int temp;
 temp=n;
 n=m;
 m=temp;
 System.out.println("After swap the n value is:"+n);
 System.out.println("After swap the m value is:"+m);
}
}
