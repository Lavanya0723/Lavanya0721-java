import java.lang.*;
import java.util.*;
class Main{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter  the n natural number:");
        int n=sc.nextInt();
        int sum=0;
        int  i=0;
        while(n>=i){
            sum=sum+i;
            i++;
        }
        System.out.println("Sum of natural numbers are:"+sum);
        
    }
}
