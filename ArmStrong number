import java.util.*;
class ArmStrong
{
public static void main(String args[])
{
 int sum=0,rem,temp;
 Scanner sc =new Scanner(System.in);
 System.out.print("Enter the ArmStrong nummber:");
 int num=sc.nextInt();
 temp=num;
 while(num!=0)
 {
  rem=num%10;
  sum=sum+rem*rem*rem;
  num=num/10;
 }
   if(sum==temp)
  {
  System.out.println(sum+"ArmStrong number");
  }

 else
 System.out.println("Not a ArmStrong number");
}
}





